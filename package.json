{
  "name": "ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "icons:desktop": "tauri icon ./public/app-icon.png -o src-tauri/icons",
    "icons:web": "tauri icon ./public/app-icon.png -o public/icons --png 16,32,48,64,128,180,192,256,512",
    "icons:all": "npm run icons:desktop && npm run icons:web",
    "tauri:dev": "tauri dev",
    "tauri:dev:linux": "WEBKIT_DISABLE_DMABUF_RENDERER=1  tauri dev",
    "tauri:build": " npm run icons:all && tauri build -- -j 8",
    "tauri:build:linux": "WEBKIT_DISABLE_DMABUF_RENDERER=1  npm run icons:all && tauri build",
    "arch:build": "cd arch && makepkg -si --noconfirm"
  },
  "dependencies": {
    "@tauri-apps/api": "2.0.0",
    "naive-ui": "^2.43.1",
    "pinia": "^3.0.3",
    "vue": "^3.5.22"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.14",
    "@tauri-apps/cli": "2.0.0",
    "@types/node": "^24.6.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.14",
    "typescript": "~5.9.3",
    "vite": "^7.1.7",
    "vue-tsc": "^3.1.0"
  }
}
