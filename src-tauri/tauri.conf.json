{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "CrabSock",
  "version": "0.1.0",
  "identifier": "dev.crabsock.app",
  "build": {
    "beforeBuildCommand": "vite build",
    "beforeDevCommand": "vite",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "additionalBrowserArgs": "--no-sandbox",
        "title": "CrabSock",
        "width": 1100,
        "height": 1000,
        "resizable": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": ["deb", "rpm", "app", "dmg","msi"],
    "resources": [
      "resources/sing-box/sing-box.exe",
      "resources/sing-box/darwin/sing-box",
      "resources/openvpn_win/openvpn_amd64",
      "resources/openvpn_win/openvpn_arm64",
      "resources/openvpn_macos/openvpn",
      "resources/openvpn_macos/openvpn_arm64",
      "resources/openvpn_macos/openvpn10"
    ],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}